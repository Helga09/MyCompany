@model ServiceItem
@{
    ViewBag.Title = Model.MetaTitle;
    ViewBag.Description = Model.MetaDescription;
    ViewBag.Keywords = Model.MetaKeywords;
}

<div>
   <h2>
       @Model.Title
   </h2>
    <h2>
        @Model.Subtitle
    </h2>
    <div>
        <img style="height:500px;width:800px;" class="img-block" src="~/images/@Model.TitleImagePath"/>
    </div>
    <div>
        @Html.Raw(Model.Text);
    </div>
</div>
